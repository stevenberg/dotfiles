Host *
    UseKeychain yes
    AddKeysToAgent yes
    VisualHostKey yes
    StrictHostKeyChecking ask
    ControlMaster auto
    ControlPath ~/.ssh/master-%r@%h:%p
    ControlPersist 60
    HashKnownHosts yes
    IdentityAgent "~/Library/Group Containers/2BUA8C4S2C.com.1password/t/agent.sock"
    IdentityFile ~/.ssh/id_ed25519
    IdentitiesOnly yes

